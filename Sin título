SELECT  JU.PKid, JU.Nombre FROM 
TblClientes Cl INNER JOIN
TblAlquiler Al ON Al.FKIdentificacion_TblClientes = Cl.PKIdentificacion 
INNER JOIN 
TblDetalleAlquiler Dt ON Dt.FKId_TblAlquiler = Al.PKId 
INNER JOIN TblJuegos JU ON JU.PKid = Dt.FKid_TblJuegos 
WHERE Cl.Edad Between 10 and 20 


SELECT rango1020.PKId, rango1020.Nombre, COUNT(rango1020.PKId) as Minimo FROM

(SELECT  JU.PKid, JU.Nombre FROM 
TblClientes Cl INNER JOIN
TblAlquiler Al ON Al.FKIdentificacion_TblClientes = Cl.PKIdentificacion 
INNER JOIN 
TblDetalleAlquiler Dt ON Dt.FKId_TblAlquiler = Al.PKId 
INNER JOIN TblJuegos JU ON JU.PKid = Dt.FKid_TblJuegos 
WHERE Cl.Edad Between 10 and 20 ) as rango1020 
GROUP BY rango1020.PKId, rango1020.Nombre


SELECT MIN(cuenta1020.cuenta) as minimo, cuenta1020.Nombre, 'edad entre 10 y 20 ' as rango   FROM (SELECT rango1020.PKId, rango1020.Nombre, COUNT(rango1020.PKId) as cuenta FROM (SELECT JU.PKid, JU.Nombre FROM TblClientes Cl INNER JOIN TblAlquiler Al ON Al.FKIdentificacion_TblClientes = Cl.PKIdentificacion INNER JOIN TblDetalleAlquiler Dt ON Dt.FKId_TblAlquiler = Al.PKId INNER JOIN TblJuegos JU ON JU.PKid = Dt.FKid_TblJuegos WHERE Cl.Edad Between 10 and 20 ) as rango1020 GROUP BY rango1020.PKId, rango1020.Nombre) as cuenta1020
UNION 
SELECT MIN(cuenta1020.cuenta) as minimo1020, cuenta1020.Nombre, 'edad entre 20 y 30 ' as rango  FROM (SELECT rango1020.PKId, rango1020.Nombre, COUNT(rango1020.PKId) as cuenta FROM (SELECT JU.PKid, JU.Nombre FROM TblClientes Cl INNER JOIN TblAlquiler Al ON Al.FKIdentificacion_TblClientes = Cl.PKIdentificacion INNER JOIN TblDetalleAlquiler Dt ON Dt.FKId_TblAlquiler = Al.PKId INNER JOIN TblJuegos JU ON JU.PKid = Dt.FKid_TblJuegos WHERE Cl.Edad Between 20 and 30 ) as rango1020 GROUP BY rango1020.PKId, rango1020.Nombre) as cuenta1020
